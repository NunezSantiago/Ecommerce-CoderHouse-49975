<h1>Upload documentation</h1>

<form action="/api/users/{{uid}}/documents" method="POST" enctype="multipart/form-data">
  <p>Identificacion</p><input type="file" name="identificacion" required>
  <br>
  <p>Comprobante de domicilio</p><input type="file" name="comprobante_domicilio" required>
  <br>
  <p>Estado de cuenta</p><input type="file" name="estado_de_cuenta" required>
  <br>
  <br>
  <input type="submit" value="Upload files">
</form>